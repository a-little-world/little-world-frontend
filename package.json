    "i18next": "^23.2.3",
    "i18next-browser-languagedetector": "^6.1.3",
    "js-cookie": "^3.0.1",
    "livekit-client": "^2.5.2",
    "lodash": "^4.17.21",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.29.4",
    "motion": "^12.4.10",
    "process": "^0.11.10",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.51.0",
    "react-i18next": "^14.0.0",
    "react-modern-drawer": "^1.2.2",
    "react-nice-avatar": "file:prebuild/react-nice-avatar-1.6.1.tgz",
