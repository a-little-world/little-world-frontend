{
  "name": "littleplanet",
  "version": "0.1.245",
  "private": true,
  "scripts": {
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "npm run lint -- --fix",
    "dev": "npx webpack serveviews/index.js --env DEV_TOOL=eval-cheap-module-source-map --env DEBUG=1 --mode development",
    "clean": "npx rimraf dist",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run copy-files:esm && npm run copy-files:cjs && npm pack",
    "build:esm": "tsc --project tsconfig.json --noCheck --outDir dist/esm",
    "build:cjs": "tsc --project tsconfig.json --noCheck --module commonjs --outDir dist/cjs",
    "copy-files:esm": "cpx \"src/**/*.css\" dist/esm && cpx \"src/images/**\" dist/esm/images && cpx \"src/locale/**\" dist/esm/locale",
    "copy-files:cjs": "cpx \"src/**/*.css\" dist/cjs && cpx \"src/images/**\" dist/cjs/images && cpx \"src/locale/**\" dist/cjs/locale",
    "changeset": "changeset",
    "version": "changeset version"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": "@a-little-world/little-world-prettier-config",
  "dependencies": {
    "@a-little-world/little-world-design-system": "^2.5.3",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@capacitor/android": "^5.6.0",
    "@capacitor/core": "^5.6.0",
    "@capacitor/ios": "^5.6.0",
    "@livekit/components-react": "file:prebuild/livekit-components-react-2.9.23.tgz",
    "@livekit/components-styles": "file:prebuild/livekit-components-styles-1.1.8.tgz",
    "@radix-ui/react-dialog": "^1.0.6",
    "@radix-ui/react-popover": "^1.0.7",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/node": "^18.11.18",
    "@types/react": "19.0.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-preset-es2015": "^6.24.1",
    "classnames": "^2.3.1",
    "date-fns": "^3.3.1",
